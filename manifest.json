{
    "manifest_version": 2,
    "name": "Moments <> Notifications",
    "version": "0.2",
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*",
                "https://www.twitter.com/*"
            ],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}

